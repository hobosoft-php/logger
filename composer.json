{
    "name": "hobosoft/logger",
    "version": "1.0.0",
    "description": "PHP Logging Library",
    "minimum-stability": "dev",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hobosoft\\Logger\\": "src/",
            "Library\\Filesystem\\Finders\\": "/var/www/projects/fdsdb/library/Filesystem/Finders/"
        }
    },
    "require-dev": {
        "pestphp/pest": "*",
        "czproject/phpdepend": "*",
        "phan/phan": "v5.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "hobosoft/config": "*"
    }
}
